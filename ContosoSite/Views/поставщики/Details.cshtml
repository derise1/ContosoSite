@model ContosoSite.Models.поставщики

@{
    ViewBag.Title = "";
}
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Nunito:wght@300&family=Readex+Pro:wght@200&display=swap" rel="stylesheet">
<link rel="stylesheet" href="~/style.css">
<h1>Поставщики</h1>
<h2>Подробнее</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ФИО
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ФИО)
        </dd>

        <dt>
            Дата рождения
        </dt>

        <dd>
            @Html.DisplayFor(model => model.дата_рождения)
        </dd>

        <dt>
            Номер телефона
        </dt>

        <dd>
            @Html.DisplayFor(model => model.номер_телефона)
        </dd>

        <dt>
            Компания
        </dt>

        <dd>
            @Html.DisplayFor(model => model.компания)
        </dd>

    </dl>
</div>
<p>
    <div class="buttTubEdit" style="width: 130px; margin-bottom: 15px;">@Html.ActionLink("Редактировать", "Edit", new { id = Model.код_поставщика })</div>
    <div class="buttTubDetails" style="width: 130px; margin-bottom: 15px;">@Html.ActionLink("Вернуться назад", "Index")</div>
</p>
