@model ContosoSite.Models.авто

@{
    ViewBag.Title = "";
}
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Nunito:wght@300&family=Readex+Pro:wght@200&display=swap" rel="stylesheet">
<link rel="stylesheet" href="~/style.css">
<h1>Авто</h1>
<h2>Подробности</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            VIN
        </dt>

        <dd>
            @Html.DisplayFor(model => model.vin)
        </dd>

        <dt>
            Марка автомобиля
        </dt>

        <dd>
            @Html.DisplayFor(model => model.марка_автомобиля)
        </dd>

        <dt>
            Модель автомобиля
        </dt>

        <dd>
            @Html.DisplayFor(model => model.модель_автомобиля)
        </dd>

        <dt>
            Год выпуска
        </dt>

        <dd>
            @Html.DisplayFor(model => model.год_выпуска)
        </dd>

        <dt>
            Вид работ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.заказы.вид_работ)
        </dd>

    </dl>
</div>
<p>
    <div class="buttTubEdit" style="width: 130px; margin-bottom: 15px;">@Html.ActionLink("Редактировать", "Edit", new { id = Model.код })</div>
    <div class="buttTubDetails" style="width: 130px; margin-bottom: 15px;">@Html.ActionLink("Вернуться назад", "Index")</div>
</p>
